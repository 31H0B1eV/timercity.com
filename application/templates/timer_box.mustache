
    <div id="{{guid}}_nav" class="tb_nav hidden">
        <a href="#/" onclick="minimizeClock({{guid}});"><span class="title">{{title}} {{type_name}}</span></a>
        <button onclick="minimizeClock({{guid}});" type="button" class="close">
            <span aria-hidden="true">&minus;</span>
            <span class="sr-only">Minimize</span>
        </button>
    </div>

    <div id="{{guid}}" class="timer_box">
        <button onclick="removeClock({{guid}});" type="button" class="close" data-dismiss="alert">
            <span aria-hidden="true">&times;</span>
            <span class="sr-only">Close</span>
        </button>
        <button onclick="minimizeClock({{guid}});" type="button" class="close">
            <span aria-hidden="true">&minus;</span>
            <span class="sr-only">Minimize</span>
        </button>
        <a id="{{guid}}_link" href="#/" class="{{type}} thumbnail">
            <div class="svg_placeholder"></div>
            <div class="laptime_display"></div>
            <div class="digital_display"></div>
            <div class="btns"></div>
            <div class="title">{{title}} {{type_name}}</div>
        </a>
    </div>